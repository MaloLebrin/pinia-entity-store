{
  "name": "@malolebrin/pinia-entity-store",
  "version": "0.2.9",
  "private": false,
  "packageManager": "pnpm@10.15.0",
  "description": "A lightweight Pinia plugin to manage relational entities in Pinia without having to learn a whole new ORM.",
  "author": {
    "name": "Malo Lebrin",
    "email": "malolebrin@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/MaloLebrin/pinia-entity-store.git"
  },
  "keywords": [
    "Pinia",
    "orm",
    "vue",
    "vue3",
    "typescript",
    "database",
    "normalizr",
    "plugin",
    "typed",
    "ts",
    "type"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist/ && tsc --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "rm -rf dist/ && npm run build",
    "postversion": "git push && git push --tags",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "@antfu/utils": "^9.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "eslint": "^9.33.0",
    "pinia": "^3.0.3",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.0.0",
    "vite": "^7.1.3",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.19"
  },
  "volta": {
    "node": "24.6.0"
  }
}
